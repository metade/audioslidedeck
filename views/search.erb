<div data-role="page"> 
  <div data-role="header">
    <h1><%= @title %></h1>
  </div>
  
  <div data-role="content">
    <ul data-role="listview" data-inset="true">
    <% @boos.each do |boo| %>
      <li><a rel=external href="/boos/<%= boo.id %>"><%= boo.title %></a></li>
    <% end %>
    </ul>
  </div>
</div>
